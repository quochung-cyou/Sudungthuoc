<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:overScrollMode="never"
    android:clickable="false"

    android:background="@color/white">

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/noconnectionani"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:lottie_autoPlay="true"
        app:lottie_fileName="12955-no-internet-connection-empty-state.json"
        app:lottie_loop="true"
        android:scaleType="fitCenter"
        app:lottie_cacheComposition="true"
        app:lottie_renderMode="hardware"
        app:lottie_repeatMode="restart"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp"></com.airbnb.lottie.LottieAnimationView>
    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/haveconnectionani"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:lottie_autoPlay="true"
        app:lottie_fileName="4171-wifi-mition.json"
        app:lottie_loop="true"
        android:scaleType="fitCenter"
        app:lottie_cacheComposition="true"
        app:lottie_renderMode="hardware"
        app:lottie_repeatMode="restart"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp"></com.airbnb.lottie.LottieAnimationView>
    <ImageView
        android:layout_width="125dp"
        android:id="@+id/logo"
        android:layout_height="125dp"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="145dp"
        android:layout_marginBottom="350dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="@drawable/launcherlogo"
        android:layout_marginLeft="145dp"></ImageView>




</androidx.constraintlayout.widget.ConstraintLayout>


